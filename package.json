{
  "name": "boilerplate-for-react-with-webpack-babel-typescript-mocha",
  "version": "1.0.0",
  "description": "Boilerplate project for react typescript webpack babel, using sass, mocha",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "babel",
    "sass",
    "starter",
    "boilerplate"
  ],
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run start-dev",
    "build": "./node_modules/.bin/webpack --config=config/webpack/dev-config.js",
    "start-dev": " ./node_modules/.bin/webpack-dev-server --content-base build --config=config/webpack/dev-config.js",
    "introspect": "apollo-codegen introspect-schema  https://fakerql.com/graphql --output PATH_TO_SCHEMA_FILE",
    "generate": "apollo-codegen generate ./src/data/**/*.tsx --schema PATH_TO_SCHEMA_FILE --target typescript --output PATH_TO_GENERATED_TYPES_FILE --add-typename --tag-name gql",
    "typegen": "npm run introspect && npm run generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TillWohlatz/boilerplate-for-react-with-webpack-babel-typescript-mocha.git"
  },
  "author": "Till Wohlatz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TillWohlatz/boilerplate-for-react-with-webpack-babel-typescript-mocha/issues"
  },
  "homepage": "https://github.com/TillWohlatz/boilerplate-for-react-with-webpack-babel-typescript-mocha#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/graphql": "^14.2.0",
    "@types/jquery": "^3.3.29",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/webpack": "^4.4.32",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "node-sass": "^4.12.0",
    "react-hot-loader": "next",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-fix-default-import-plugin": "^1.0.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-codegen": "^0.20.2",
    "apollo-link-http": "^1.5.15",
    "ejs": "^2.6.1",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "jquery": "^3.4.1",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.8.6",
    "react-emotion": "^10.0.0",
    "react-responsive": "^7.0.0",
    "react-router-dom": "^5.0.1",
    "reactstrap": "^8.0.0",
    "tslint": "^5.17.0"
  }
}
